         <script>
              /* Codigo necesaio para funcionamiento */
              let numSemanas = document.getElementById('capacitacion').children.length;
                 let semanas = document.querySelectorAll('.semana');
                 numSemanas -= 5;
                 semanas.forEach((semana, idx) => {
                     semana.innerText = "0"+(idx+1) ;
                 });
                 if(numSemanas <= 4){
                     document.getElementById('unaFila').style.display = 'block';
                 }else if(numSemanas <= 8){
                    document.getElementById('dosFilas').style.display = 'block';
                 }else if(numSemanas <= 12){
                     document.getElementById('tresFilas').style.display = 'block';
                 }
                 else if(numSemanas <= 16){
                     document.getElementById('cuatroFilas').style.display = 'block';
                 }
       
                    /* Codigo necesaio para funcionamiento */
              let numSemanasmax = document.getElementById('capacitacionmax').children.length;
                 
                 let semanasmax = document.querySelectorAll('.semanamax');
                 numSemanasmax -= 8;
                 semanasmax.forEach((semana, idx) => {
                     semana.innerText = "0"+(idx+1) ;
                 });
                 
                 if(numSemanasmax <= 4){
                     document.getElementById('unaFilaMax').style.display = 'block';
                 }else if(numSemanasmax <= 8){
                     document.getElementById('dosFilasMax').style.display = 'block';
                 }else if(numSemanasmax<= 12){
                     document.getElementById('tresFilasMax').style.display = 'block';
                 }else if(numSemanasmax<= 16){
                     document.getElementById('cuatroFilasMax').style.display = 'block';
                 }else if(numSemanasmax<= 20){
                     document.getElementById('cincoFilasMax').style.display = 'block';
